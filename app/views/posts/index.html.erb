<h1>All Posts</h1>
<% @posts.each do |post| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <% raise "Found post without id: #{post.inspect}" unless post.id %>
        <%= link_to post.title, post_path(post)%></p>
      </h4>
    </div>
  </div>
<% end %>